<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Book List</title>
</head>
<body>
    <h1>Books</h1>
    <ul>
        <% books.forEach(function(book) { %>
            <li>
                <h2><%= book.title %></h2>
                <p>Author: <%= book.authorName %></p>
                <p>Genre: <%= book.genre %></p>
                <p>Publisher: <%= book.publisher %>, Published Date: <%= book.publishDate %></p>
                <p>Quantity: <%= book.quantity %>, Available: <%= book.availableQuantity %></p>
            </li>
        <% }); %>
    </ul>
</body>
</html>